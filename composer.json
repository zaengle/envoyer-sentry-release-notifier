{
  "name": "zaengle/envoyer-sentry-release-notifier",
  "description": "A tool to help package release commits for Sentry.",
  "license": "MIT",
  "keywords": ["laravel", "envoyer", "sentry"],
  "authors": [
    {
      "name": "Zaengle",
      "email": "info@zaengle.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "illuminate/support": "5.*|6.*|7.*|8.*",
    "illuminate/console": "5.*|6.*|7.*|8.*"
  },
  "require-dev": {
    "mockery/mockery": ">=0.9.9",
    "phpunit/phpunit": ">=4.1",
    "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0"
  },
  "autoload": {
    "psr-4": {
      "Zaengle\\EnvoyerSentryReleaseNotifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zaengle\\EnvoyerSentryReleaseNotifier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zaengle\\EnvoyerSentryReleaseNotifier\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
