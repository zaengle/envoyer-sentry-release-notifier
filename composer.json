{
  "name": "zaengle/envoyer-sentry-release-notifier",
  "description": "A tool to help package release commits for Sentry.",
  "license": "MIT",
  "keywords": ["laravel", "envoyer", "sentry"],
  "authors": [
    {
      "name": "Zaengle",
      "email": "info@zaengle.com"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "illuminate/support": "5.*|6.*|7.*|8.*|9.*",
    "illuminate/console": "5.*|6.*|7.*|8.*|9.*"
  },
  "autoload": {
    "psr-4": {
      "Zaengle\\EnvoyerSentryReleaseNotifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zaengle\\EnvoyerSentryReleaseNotifier\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zaengle\\EnvoyerSentryReleaseNotifier\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
